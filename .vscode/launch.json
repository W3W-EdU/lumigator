{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Demo env",
            "python": "${workspaceFolder}/lumigator/demo/demoenv/bin/python",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}/lumigator/demo",
